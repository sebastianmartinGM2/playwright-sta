{
  "name": "playwright",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "PWDEBUG=1 playwright test",
    "test:visual": "playwright test --project demoblaze-chromium --grep @visual",
    "test:testim": "playwright test --project testim-chromium",
    "test:mystapp:setup": "playwright test --project mystapp-setup",
    "test:mystapp": "playwright test --project mystapp-chromium",
    "test:mystapp:service-vehicles": "playwright test tests/mystapp/service-vehicles.spec.ts --project mystapp-chromium",
    "test:mystapp:service-vehicles:perf": "npm run test:mystapp:service-vehicles && npm run perf:mystapp:md",
    "perf:mystapp:md": "node scripts/mystapp-perf-summary.mjs",
    "report": "playwright show-report"
  },
  "keywords": [],
  "author": "",
