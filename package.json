  "type": "commonjs",
  "devDependencies": {
    "@applitools/eyes-playwright": "^1.44.2",
    "@playwright/test": "^1.58.1",
    "dotenv": "^17.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
